generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Libro {
  id          Int     @id @default(autoincrement())
  titulo      String
  autor       String
  descripcion String
  genero      String
  imagen      String
  categoria   String
  slug        String  @unique
  cantidad    Int
  precio      Int
}


model User {
  id String @id
  username String @unique
  password String
}

